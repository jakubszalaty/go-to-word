{
    "name": "go-to-word",
    "displayName": "Go to Word",
    "description": "Go to any word in current file. Like Sublime text Goto #",
    "version": "0.2.3",
    "publisher": "jakubszalaty",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "search",
        "sublime",
        "word",
        "go to"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/jakubszalaty/go-to-word"
    },
    "activationEvents": [
        "onCommand:goToWord.find"
    ],
    "icon": "images/got-to-word.png",
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "goToWord.find",
                "title": "Go to word"
            }
        ]
    },
    "configuration": {
        "title": "Go to word configuration",
        "type": "object",
        "properties": {
            "goToWord.matchRegExp": {
                "type": "string",
                "default": "((?![\\. <>`'\"\\!@#\\$%\\^&\\*\\(\\)=\\+]).)+",
                "description": "Regexp for search words in file."
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.0.25",
        "@types/ramda": "^0.24.7",
        "mocha": "^3.5.0",
        "typescript": "^2.4.2",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "ramda": "^0.24.1"
    }
}